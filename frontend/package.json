    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "29.0.1",
    "jest-cli": "29.0.1",
    "jest-config": "29.0.1",
    "jest-environment-node": "29.0.1",
    "jest-extended": "^4.0.1",
    "jsdom": "^22.1.0",
    "mocha": "^10.2.0",
    "mocha-jsdom": "^2.0.0",
    "node-localstorage": "^3.0.5",
    "react-devtools": "^4.28.0",
