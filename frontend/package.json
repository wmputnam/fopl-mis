{
  "name": "fopl-list",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/frontend/App.jsx",
  "scripts": {
    "start": "react-scripts start --profile",
    "start:dev": "react-scripts start --profile",
    "build": "react-scripts build",
    "test": "npm run test:mocha",
    "test:dev": "npm run test:mocha",
    "test:react": "react-scripts test",
    "test:jest": "jest",
    "test:unit": "mocha -r ./test/helpers.js -r ./test/dom.js -r ./test/hook.js -r ts-node/register '**/*.test.ts' --unhandled-rejections=strict",
    "test:unit2": "mocha --unhandled-rejections=strict",
    "test:mocha": "scripts/be-status && scripts/clean-dist && tsc --showConfig && tsc -outDir './dist' && mocha -r node_modules/@esbuild-kit/cjs-loader -r ./test/helpers.js -r node-localstorage/register -r ./test/dom.js",
    "test:ci": "CI=true react-scripts test --watchAll=false --verbose --debug --detectOpenHandles --forceExit --runInBand --logHeapUsage --watchman=false --no-watchman--testEnvironmentOptions='{\"html\": \"<html></html>\"}'",
    "test:tsmocha": "DEBUG=* ts-mocha",
    "eject": "react-scripts eject",
    "lint": "tsc --noEmit",
    "lint:old": "./node_modules/.bin/eslint",
    "ts-node:run": "node ./src/@components/DataLoader.test.ts"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/flat": "^5.0.2",
    "@types/jest": "^29.5.3",
    "@types/node": "^16.18.41",
    "@types/react": "^18.2.20",
    "@types/react-currency-format": "^1.0.0",
    "@types/react-dom": "^18.2.7",
    "@types/react-modal": "^3.16.1",
    "axios": "^1.4.0",
    "axios-hooks": "^4.0.0",
    "caniuse-lite": "^1.0.30001594",
    "debug": "^4.3.4",
    "flat": "^5.0.2",
    "nanoid": "^4.0.2",
    "packages": "../packages/member-shared",
    "pnpm": "^8.6.12",
    "react": "^18.2.0",
    "react-currency-format": "^1.1.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-scripts": "5.0.1",
    "superagent": "^8.1.2",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.8",
    "typescript": "^4.9.5",
    "use-axios-client": "^2.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
