    "@esbuild-kit/core-utils": "^3.2.2",
    "@esbuild-kit/esm-loader": "^2.5.5",
    "@jest/core": "29.0.1",
    "@jest/globals": "^29.6.4",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.5",
    "@types/debug": "^4.1.8",
    "@types/enzyme": "^3.10.13",
    "@types/enzyme-to-json": "^1.5.4",
    "@types/expect": "^24.3.0",
    "@types/jsdom": "^21.1.1",
    "@types/mocha": "^10.0.1",
